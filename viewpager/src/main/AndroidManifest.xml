<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.donfyy.viewpager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.donfyy.viewpager.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.donfyy.viewpager.wrap.WrapActivity"/>
        <activity android:name="com.donfyy.viewpager.lazyloading.lazy3.LazyMain3Activity" />
        <activity android:name="com.donfyy.viewpager.lazyloading.lazy2.LazyMain2Activity" />
        <activity android:name="com.donfyy.viewpager.lazyloading.LazyMainActivity" />
    </application>

</manifest>